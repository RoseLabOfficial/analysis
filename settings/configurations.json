{
    "inputdir": "./inputs",
    "outputdir": "./outputs",
    "skipfiles": [],
    "userfiles": [],
    "filters": {
        "membrane_potentials": {
            "passband": 200,
            "ripple": 0.01,
            "stopband": 400,
            "attenuation": 80
        },
        "membrane_currents": {
            "passband": 40,
            "ripple": 0.01,
            "stopband": 60,
            "attenuation": 80
        },
        "activation_currents": {
            "passband": 40,
            "ripple": 0.01,
            "stopband": 60,
            "attenuation": 80
        }
    },
    "optimization": {
        "level": 1
    },
    "loggers": {
        "version": 1,
        "handlers": {
            "console": {
                "class": "logging.StreamHandler",
                "level": "DEBUG",
                "formatter": "detailed"
            },
            "file": {
                "class": "logging.FileHandler",
                "filename": "./logs/Analyzer.log",
                "level": "DEBUG",
                "formatter": "detailed"
            }
        },
        "formatters": {
            "detailed": {
                "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
            }
        },
        "loggers": {
            "Analyzer": {
                "handlers": ["file"],
                "level": "DEBUG"
            }
        },
        "root": {
            "level": "DEBUG",
            "handlers": ["console"]
        }
    }
}